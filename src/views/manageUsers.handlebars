<h1>Renderizar usuarios</h1>

  <table>
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Correo</th>
                <th>Rol</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr>
                <td>{{first_name}}</td>
                <td>{{email}}</td>
                <td>{{role}}</td>
                <td>
                    <button onclick="deleteUser('{{_id}}')">Eliminar</button>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>

<script src="/js/deleteUser.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
